<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <OutputType>Library</OutputType>
    <Authors>Hussein Al Bayati</Authors>
    <Copyright>Hussein Al Bayati</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/H7O/Com.H.Net.Ssh</PackageProjectUrl>
    <RepositoryUrl>https://github.com/H7O/Com.H.Net.Ssh</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>ssh,sftp</PackageTags>
    <Description>Wrapper to SSH.NET API by Renci. 
Introduce minor new functionality.</Description>
    <PackageReleaseNotes>Initial release</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="SSH.NET" Version="2020.0.1" />
  </ItemGroup>

</Project>
